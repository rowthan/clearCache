{
   "name": "__MSG_extensionName__",
   "options_page": "options.html",
   "permissions": [ "browsingData", "tabs", "cookies", "\u003Call_urls>" ,"contextMenus"],
   "short_name": "__MSG_extensionName__",
   "description": "__MSG_extensionDescription__",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.1",
   "default_locale": "en",
   "background": {
      "scripts": [ "js/jquery.min.js", "js/load-default-options.js", "js/background.js","js/contextMenu.js" ,"js/clearCache.js"]
   },
   "browser_action": {
      "default_icon": {
         "19": "img/icon_19.png",
         "38": "img/icon_38.png"
      },
      "default_title": "__MSG_extensionName__"
   },
   "commands": {
      "_execute_browser_action":{
         "suggested_key": {
            "default": "Ctrl+Shift+X"
         },
         "description": "__MSG_extensionCommand__"
      }
   },
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "icons": {
      "128": "img/icon_128.png",
      "16": "img/icon_16.png",
      "48": "img/icon_48.png"
   },

   "manifest_version": 2,
   "minimum_chrome_version": "22"
}
